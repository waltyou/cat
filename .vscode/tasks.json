{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "vscode-extension:esbuild",
      "type": "shell",
      "command": "node",
      "args": [
        "./scripts/esbuild.js"
      ],
      "problemMatcher": "$tsc",
      "presentation": {
        "reveal": "always"
      },
      "group": "build",
      "options": {
        "cwd": "${workspaceFolder}/extensions/vscode"
      }
    }
  ]
}